<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/apps/gwd/use_metacity_theme</key>
			<applyto>/apps/gwd/use_metacity_theme</applyto>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Use Metacity theme.</short>
				<long>Use the current Metacity theme.</long>
			</locale>
		</schema>
	</schemalist>
</gconfschemafile>

