<compiz>
    <plugin name="plane">
	<_short>Desktop Plane</_short>
	<_long>Place windows on a plane</_long>
	<feature>largedesktop</feature>
	<deps>
	    <relation type="before">
		<plugin>switcher</plugin>
		<plugin>scale</plugin>
	    </relation>
	</deps>
	<display>
	    <option name="plane_left_key" type="key">
		<_short>Plane Left</_short>
		<_long>Plane left</_long>
		<default>&lt;Control&gt;&lt;Alt&gt;Left</default>
	    </option>
	    <option name="plane_right_key" type="key">
		<_short>Plane Right</_short>
		<_long>Plane right</_long>
		<default>&lt;Control&gt;&lt;Alt&gt;Right</default>
	    </option>
	    <option name="plane_down_key" type="key">
		<_short>Plane Down</_short>
		<_long>Plane down</_long>
		<default>&lt;Control&gt;&lt;Alt&gt;Down</default>
	    </option>
	    <option name="plane_up_key" type="key">
		<_short>Plane Up</_short>
		<_long>Plane up</_long>
		<default>&lt;Control&gt;&lt;Alt&gt;Up</default>
	    </option>
	    <option name="plane_to_1_key" type="key">
		<_short>Plane To Face 1</_short>
		<_long>Plane to face 1</_long>
	    </option>
	    <option name="plane_to_2_key" type="key">
		<_short>Plane To Face 2</_short>
		<_long>Plane to face 2</_long>
	    </option>
	    <option name="plane_to_3_key" type="key">
		<_short>Plane To Face 3</_short>
		<_long>Plane to face 3</_long>
	    </option>
	    <option name="plane_to_4_key" type="key">
		<_short>Plane To Face 4</_short>
		<_long>Plane to face 4</_long>
	    </option>
	    <option name="plane_to_5_key" type="key">
		<_short>Plane To Face 5</_short>
		<_long>Plane to face 5</_long>
	    </option>
	    <option name="plane_to_6_key" type="key">
		<_short>Plane To Face 6</_short>
		<_long>Plane to face 6</_long>
	    </option>
	    <option name="plane_to_7_key" type="key">
		<_short>Plane To Face 7</_short>
		<_long>Plane to face 7</_long>
	    </option>
	    <option name="plane_to_8_key" type="key">
		<_short>Plane To Face 8</_short>
		<_long>Plane to face 8</_long>
	    </option>
	    <option name="plane_to_9_key" type="key">
		<_short>Plane To Face 9</_short>
		<_long>Plane to face 9</_long>
	    </option>
	    <option name="plane_to_10_key" type="key">
		<_short>Plane To Face 10</_short>
		<_long>Plane to face 10</_long>
	    </option>
	    <option name="plane_to_11_key" type="key">
		<_short>Plane To Face 11</_short>
		<_long>Plane to face 11</_long>
	    </option>
	    <option name="plane_to_12_key" type="key">
		<_short>Plane To Face 12</_short>
		<_long>Plane to face 12</_long>
	    </option>
	</display>
    </plugin>
</compiz>
